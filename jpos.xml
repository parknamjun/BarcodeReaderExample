<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE JposEntries PUBLIC "-//JavaPOS//DTD//EN"
                             "jpos/res/jcl.dtd">
<JposEntries>
    <JposEntry logicalName="QS6000">
        <creation factoryClass="com.dls.jpos.service.DLSScannerInstanceFactory"
                  serviceClass="com.dls.jpos.service.DLSScannerService"/>
        <vendor name="DLA" url="http://www.adc.datalogic.com"/>
        <jpos category="Scanner" version="1.14"/>
        <product description="ScannerService" name="ScannerService" url="http://www.adc.datalogic.com"/>
        <!--Other non JavaPOS required property (mostly vendor properties and bus specific properties i.e. RS232 )-->
        <prop name="portName" value="COM8"/>
        <prop name="deviceBus" type="String" value="RS232"/>
        <!-- Additional properties as needed -->
    </JposEntry>
    <JposEntry logicalName="DL-Magellan-1500i-USB-OEM">
        <creation factoryClass="com.dls.jpos.service.DLSScannerInstanceFactory" serviceClass="com.dls.jpos.service.DLSScannerService"/>
        <vendor name="DLA" url="http://www.adc.datalogic.com"/>
        <jpos category="Scanner" version="1.14"/>
        <product description="ScannerService" name="ScannerService" url="http://www.adc.datalogic.com"/>
        <!--Other non JavaPOS required property (mostly vendor properties and bus specific properties i.e. RS232 )-->
        <prop name="canNotifyPowerChange" type="String" value="True"/>
        <prop name="canUpdateFirmware" type="String" value="True"/>
        <prop name="usage" type="String" value="4a00"/>
        <prop name="baudRate" value="9600"></prop>
        <prop name="portName" value="COM8"></prop>
        <prop name="deviceBus" type="String" value="RS232"/>

        <!-- Automatic input options -->
        <prop name="autoDisable" type="String" value="False"/>
        <prop name="dataEventEnabled" type="String" value="True"/><!-- green spot setting 후 사용할 수 있음-->
    </JposEntry>
</JposEntries>
